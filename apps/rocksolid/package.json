{
  "name": "rocksolid",
  "type": "module",
  "scripts": {
    "build": "solid-start build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "coverage": "vitest run --coverage",
    "dev": "solid-start dev --port 3005",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "prepare": "panda codegen --silent",
    "start": "solid-start start",
    "test": "vitest run",
    "test-ui": "vitest --ui",
    "test-watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@pixelstack/prettier-config",
  "eslintConfig": {
    "extends": [
      "@pixelstack/eslint-config/base",
      "@pixelstack/eslint-config/solid"
    ],
    "ignorePatterns": [
      "node_modules",
      ".next",
      ".turbo",
      "src/styled-system",
      "src/*.test.tsx"
    ],
    "root": true
  },
  "dependencies": {
    "@ark-ui/solid": "^1.2.0",
    "@auth/core": "^0.19.0",
    "@auth/solid-start": "^0.1.9",
    "@fontsource-variable/plus-jakarta-sans": "^5.0.19",
    "@park-ui/presets": "^0.19.0",
    "@pixelstack/api": "workspace:*",
    "@pixelstack/auth": "workspace:*",
    "@pixelstack/db": "workspace:*",
    "@solid-mediakit/auth": "^1.0.3",
    "@supabase/supabase-js": "^2.39.1",
    "@tanstack/solid-query": "^5.17.0",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "lucide-solid": "^0.303.0",
    "mapbox-gl": "^3.0.1",
    "solid-map-gl": "^1.10.3",
    "solid-start-trpc": "^0.0.16",
    "solid-supabase": "^0.5.0",
    "solid-trpc": "0.1.0-sssr.8",
    "superjson": "2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.23.0",
    "@pixelstack/eslint-config": "workspace:*",
    "@pixelstack/prettier-config": "workspace:*",
    "@pixelstack/tsconfig": "workspace:*",
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.10.5",
    "@solidjs/testing-library": "^0.8.5",
    "@testing-library/jest-dom": "^6.1.6",
    "@types/jsdom": "^21.1.6",
    "@types/mapbox-gl": "^2.7.19",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^1.1.1",
    "dotenv-cli": "^7.3.0",
    "jsdom": "^23.0.1",
    "solid-js": "^1.8.7",
    "solid-start": "^0.3.10",
    "solid-start-node": "^0.3.10",
    "typescript": "^5.3.3",
    "undici": "^6.2.1",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.1"
  }
}
